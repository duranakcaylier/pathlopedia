    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>/</url-pattern>
        <url-pattern>/s/*.html</url-pattern>
        <url-pattern>/s/*.js</url-pattern>
        <url-pattern>/s/*.png</url-pattern>
    </servlet-mapping>
