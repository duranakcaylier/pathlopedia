<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Pathlopedia</display-name>

    <!-- /f/attachment/comment/add -->
    <servlet>
        <servlet-name>AttachmentCommentAddServlet</servlet-name>
        <servlet-class>com.pathlopedia.servlet.AttachmentCommentAddServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AttachmentCommentAddServlet</servlet-name>
        <url-pattern>/f/attachment/comment/add</url-pattern>
    </servlet-mapping>

    <!-- /f/attachment/get -->
    <servlet>
        <servlet-name>AttachmentGetServlet</servlet-name>
        <servlet-class>com.pathlopedia.servlet.AttachmentGetServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetAttachmentServlet</servlet-name>
        <url-pattern>/f/attachment/get</url-pattern>
    </servlet-mapping>

